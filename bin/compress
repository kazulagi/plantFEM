#!/bin/sh
cp CMakeLists.txt ./bin/
rm hostfile
rm ./GetPixcel*.py
rm ./DBCGetPixcelSize*.py
rm *.o
rm *.so
rm *.mod
rm *.txt
rm *.vtk
rm *.scf
rm *.json
rm *.scf~
rm fort.*
rm *.ipynb
rm *.mesh
rm PySiCroF.py
mv *.f90 log/
mv *.geo RESULTS/
mv *.msh RESULTS/
rm *.pos 
rm *.msh 
rm *.stl 
mv GetSurface* RESULTS/
mv *.txt~ RESULTS/
mv *.blend RESULTS/
